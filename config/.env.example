# Withings API Credentials
WITHINGS_CLIENT_ID=your_withings_client_id
WITHINGS_CLIENT_SECRET=your_withings_secret
WITHINGS_CALLBACK_URI=your_callback_uri

# Omron API Credentials
OMRON_CLIENT_ID=your_omron_client_id
OMRON_CLIENT_SECRET=your_omron_secret

# Google Calendar Settings
GOOGLE_CALENDAR_ID=your_calendar_id

# Google Home Settings
GOOGLE_HOME_PROJECT_ID=your_project_id
GOOGLE_HOME_DEVICE_ID=your_device_id
GOOGLE_HOME_LANGUAGE='en-US'

# Redis Cache (Optional)
REDIS_URL=redis://localhost:6379

# Notification Settings
NOTIFICATION_LEVEL=INFO  # INFO, WARNING, ERROR
ENABLE_GOOGLE_HOME=true
ENABLE_TELEGRAM=false

# Application Settings
SYNC_INTERVAL=60  # seconds
DEBUG_MODE=false
TIMEZONE=UTC 